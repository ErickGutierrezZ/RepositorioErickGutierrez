while True:
    p=False
    n = int(input("Escribe cuantos numeros quieres"))
    if(n<0):
        p
    p=True
    for x in range (n):
            num=int(input("Escribe un nÃºmero divisble de 4 "))
            if(num<4):
                print("Numeros mayores a 4")
                p
            if(num%4==0):
                print(num,"YES")
            else:
                print("NO")
                
